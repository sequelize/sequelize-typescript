{
  "name": "sequelize-typescript",
  "version": "2.0.0-beta.1",
  "description": "Decorators and some other features for sequelize",
  "scripts": {
    "build": "tsc",
    "test": "mocha test/**/*.spec.ts",
    "cover": "nyc mocha test/**/*.spec.ts",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobinBuschmann/sequelize-typescript.git"
  },
  "keywords": [
    "orm",
    "object relational mapper",
    "sequelize",
    "typescript",
    "decorators",
    "mysql",
    "sqlite",
    "postgresql",
    "postgres",
    "mssql"
  ],
  "author": "Robin Buschmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RobinBuschmann/sequelize-typescript/issues"
  },
  "homepage": "https://github.com/RobinBuschmann/sequelize-typescript#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/browser/index.js",
  "dependencies": {
    "glob": "7.1.6"
  },
  "devDependencies": {
    "@types/chai": "4.2.14",
    "@types/chai-as-promised": "7.1.3",
    "@types/chai-datetime": "0.0.36",
    "@types/lodash": "4.14.167",
    "@types/mocha": "8.2.0",
    "@types/node": "14.14.20",
    "@types/prettyjson": "0.0.29",
    "@types/sinon": "9.0.10",
    "@types/sinon-chai": "3.2.5",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-datetime": "1.7.0",
    "codecov": "3.8.1",
    "copyfiles": "2.4.1",
    "has-flag": "4.0.0",
    "lodash": "4.17.20",
    "mocha": "8.2.1",
    "moment": "2.29.1",
    "mysql2": "2.2.5",
    "nyc": "15.1.0",
    "prettyjson": "1.2.1",
    "reflect-metadata": "0.1.13",
    "sequelize": "6.3.5",
    "sinon": "9.2.3",
    "sinon-chai": "3.5.0",
    "source-map-support": "0.5.19",
    "sqlite3": "5.0.1",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.1.3",
    "uuid-validate": "0.0.3"
  },
  "peerDependencies": {
    "@types/bluebird": "*",
    "@types/node": "*",
    "@types/validator": "*",
    "reflect-metadata": "*",
    "sequelize": "^6.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "nyc": {
    "lines": 85,
    "statements": 85,
    "functions": 85,
    "branches": 85,
    "include": [
      "src"
    ],
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "report-dir": "./coverage"
  }
}
